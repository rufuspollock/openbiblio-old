@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix fresnel:  <http://www.w3.org/2004/09/fresnel#> .
@prefix cs:       <http://purl.org/vocab/changeset/schema#> .
@prefix gr:       <http://bibliographica.org/schema/graph#> .
@prefix dc:       <http://purl.org/dc/terms/> .
@prefix :         <http://bibliographica.org/lenses/changeset#> .

<http://bibliographica.org/lens/changeset> rdfs:label "Changeset" .

:changeSetDefaultLens a fresnel:Lens ;
	fresnel:purpose fresnel:defaultLens ;
	fresnel:classLensDomain cs:ChangeSet ;
	fresnel:group :csGroup ;
	fresnel:showProperties (
		cs:changeReason
		cs:createdDate
		cs:creatorName
	) .

:csGroup a fresnel:Group .

:changeSetFormat a fresnel:Format ;
	fresnel:classFormatDomain cs:ChangeSet ;
	fresnel:group :csGroup .

:createdDateFormat a fresnel:Format ;
	fresnel:propertyFormatDomain cs:createdDate ;
	fresnel:label "Date" ;
	fresnel:group :csGroup .

:creatorName a fresnel:Format ;
	fresnel:propertyFormatDomain cs:creatorName ;
	fresnel:label "Creator" ;
	fresnel:group :csGroup .

:changeReason a fresnel:Format ;
	fresnel:propertyFormatDomain cs:changeReason ;
	fresnel:label fresnel:none ;
	fresnel:propertyStyle "nolabel"^^fresnel:styleClass ;
	fresnel:group :csGroup .
